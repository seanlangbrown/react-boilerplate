{
  "parser": "babel-eslint",
  "parserOptions": {},
  "env": {
    "browser": true
  },
  "plugins": [
    "flowtype",
    "react",
    "react-redux"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:flowtype/recommended",
    "plugin:react/recommended",
    "plugin:react-redux/recommended"
  ],
  "settings": {
    "react": {
      "version": "latest"
    },
  },
  "rules": {
    "no-unused-vars": 0
  }
}
