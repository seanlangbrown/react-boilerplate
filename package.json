{
  "name": "react-boilerplate",
  "version": "0.0.0-semantic-release",
  "description": "React 16.5.2 + babel 7 + jest + webpack 4 + flow + eslint + esdoc + live editing boilerplate.",
  "scripts": {
    "start": "npx webpack-dev-server --mode development --open",
    "test": "npx jest",
    "pretest": "npx run-s clean build",
    "test:badges": "npx run-s test:coverage test:badgesOnly",
    "test:badgesOnly": "npx jest-coverage-badges",
    "test:coverage": "npx jest --coverage",
    "test:watch": "npx jest --watch",
    "build": "npx webpack --mode production",
    "clean": "npx rimraf dist doc",
    "commit": "npx git-cz",
    "dev": "npx webpack --mode development",
    "esdoc": "npx esdoc",
    "flow": "npx flow",
    "lint": "better-npm-run lint",
    "release": "npx run-s release:check release:sr",
    "release:check": "npx run-s clean build test:badges flow lint",
    "release:sr": "npx semantic-release"
  },
  "keywords": [
    "boilerplate",
    "starter",
    "kit",
    "starter kit",
    "template",
    "quickstart",
    "react",
    "babel",
    "eslint",
    "esdoc",
    "jest",
    "webpack",
    "commitizen"
  ],
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "better-npm-run": "^0.1.1",
    "commitizen": "^3.0.2",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "esdoc": "^1.1.0",
    "esdoc-brand-plugin": "^1.0.1",
    "esdoc-flow-type-plugin": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.82.0",
    "flow-typed": "^2.4.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-coverage-badges": "^1.0.0",
    "mini-css-extract-plugin": "^0.4.3",
    "npm-run-all": "^4.1.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.17",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "author": "Ziv Barber",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "main": "src/index.js",
  "betterScripts": {
    "lint": {
      "command": "npx eslint \"**/*.{js,jsx}\"",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "release": {
    "prepare": [],
    "publish": [
      "@semantic-release/github"
    ],
    "npmPublish": false,
    "verifyConditions": [
      "@semantic-release/github"
    ]
  }
}